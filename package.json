{
  "name": "@form8ion/utils-cli",
  "description": "cli for various tools for the organization",
  "license": "MIT",
  "version": "0.0.0-semantically-released",
  "bin": {
    "form8ion-utils": "bin/form8ion-utils.js"
  },
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "bin/"
  ],
  "repository": "form8ion/utils-cli",
  "bugs": "https://github.com/form8ion/utils-cli/issues",
  "homepage": "https://npm.im/@form8ion/utils-cli",
  "author": "Matt Travi <npm@travi.org> (https://matt.travi.org/)",
  "scripts": {
    "test": "npm-run-all --print-label --parallel lint:* --parallel test:*",
    "test:unit": "nyc run-s test:unit:base",
    "test:unit:base": "DEBUG=any mocha --recursive test/unit",
    "coverage:report": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "test:integration": "run-s 'test:integration:base -- --profile noWip'",
    "test:integration:base": "DEBUG=any cucumber-js test/integration --profile base",
    "test:integration:debug": "DEBUG=test run-s test:integration",
    "test:integration:wip": "run-s 'test:integration:base -- --profile wip'",
    "lint:js": "eslint . --cache",
    "lint:md": "remark . --frail",
    "lint:sensitive": "ban",
    "lint:travis": "travis-lint .travis.yml",
    "clean": "rimraf ./bin",
    "prebuild": "run-s clean",
    "build": "npm-run-all --print-label --parallel build:*",
    "build:js": "rollup --config",
    "watch": "run-s 'build:js -- --watch'",
    "prepack": "run-s build"
  },
  "devDependencies": {
    "@babel/register": "7.4.4",
    "@form8ion/babel-preset": "1.2.1",
    "@form8ion/commitlint-config": "1.0.3",
    "@form8ion/eslint-config": "1.1.0",
    "@form8ion/eslint-config-mocha": "1.2.0",
    "@form8ion/remark-lint-preset": "1.0.0",
    "@travi/any": "2.0.0",
    "ban-sensitive-files": "1.9.2",
    "chai": "4.2.0",
    "codecov": "3.5.0",
    "cucumber": "5.1.0",
    "cz-conventional-changelog": "2.1.0",
    "husky": "2.3.0",
    "mocha": "6.1.4",
    "npm-run-all": "4.1.5",
    "nyc": "14.1.1",
    "remark-cli": "6.0.1",
    "remark-preset-lint-travi": "1.2.0",
    "rimraf": "2.6.3",
    "rollup": "1.12.5",
    "rollup-plugin-auto-external": "2.0.0",
    "rollup-plugin-executable": "1.4.5",
    "rollup-plugin-json": "4.0.0",
    "sinon": "7.3.2",
    "travis-lint": "1.0.0"
  },
  "dependencies": {
    "@travi/github-scaffolder": "4.0.1",
    "@travi/javascript-scaffolder": "6.21.3",
    "@travi/project-scaffolder": "6.4.3",
    "@travi/travis-scaffolder-javascript": "1.12.0",
    "update-notifier": "3.0.0",
    "yargs": "13.2.4"
  }
}
