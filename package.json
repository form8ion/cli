{
  "name": "@form8ion/utils-cli",
  "description": "cli for various tools for the organization",
  "license": "MIT",
  "version": "0.0.0-semantically-released",
  "bin": {
    "form8ion-utils": "bin/form8ion-utils.js"
  },
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=12"
  },
  "files": [
    "bin/"
  ],
  "repository": "form8ion/utils-cli",
  "bugs": "https://github.com/form8ion/utils-cli/issues",
  "homepage": "https://npm.im/@form8ion/utils-cli",
  "author": "Matt Travi <npm@travi.org> (https://matt.travi.org/)",
  "scripts": {
    "test": "npm-run-all --print-label --parallel lint:* --parallel test:*",
    "test:unit": "nyc run-s test:unit:base",
    "test:unit:base": "DEBUG=any mocha 'src/**/*-test.js'",
    "coverage:report": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "test:integration": "run-s 'test:integration:base -- --profile noWip'",
    "test:integration:base": "DEBUG=any cucumber-js test/integration --profile base",
    "test:integration:debug": "DEBUG=nock.* run-s test:integration",
    "test:integration:wip": "run-s 'test:integration:base -- --profile wip'",
    "test:integration:focus": "run-s 'test:integration:base -- --profile focus'",
    "lint:js": "eslint . --cache",
    "lint:md": "remark . --frail",
    "lint:sensitive": "ban",
    "lint:engines": "check-engine",
    "lint:gherkin": "gherkin-lint",
    "lint:peer": "npm ls >/dev/null",
    "clean": "rimraf ./bin",
    "prebuild": "run-s clean",
    "build": "npm-run-all --print-label --parallel build:*",
    "build:js": "rollup --config",
    "watch": "run-s 'build:js -- --watch'",
    "prepack": "run-s build"
  },
  "devDependencies": {
    "@babel/node": "7.12.13",
    "@babel/register": "7.12.13",
    "@form8ion/babel-preset": "1.6.42",
    "@form8ion/commitlint-config": "1.0.14",
    "@form8ion/eslint-config": "1.7.6",
    "@form8ion/eslint-config-cucumber": "1.4.0",
    "@form8ion/eslint-config-mocha": "1.2.9",
    "@form8ion/remark-lint-preset": "1.0.11",
    "@travi/any": "2.0.15",
    "ban-sensitive-files": "1.9.15",
    "chai": "4.3.0",
    "check-engine": "1.10.0",
    "codecov": "3.8.1",
    "cucumber": "6.0.5",
    "cz-conventional-changelog": "3.3.0",
    "gherkin-lint": "4.1.3",
    "http-status-codes": "2.1.4",
    "husky": "5.0.8",
    "mocha": "8.2.1",
    "mock-fs": "4.13.0",
    "mz": "2.7.0",
    "nock": "13.0.7",
    "npm-run-all": "4.1.5",
    "nyc": "15.1.0",
    "remark-cli": "9.0.0",
    "rimraf": "3.0.2",
    "rollup": "2.38.4",
    "rollup-plugin-auto-external": "2.0.0",
    "rollup-plugin-executable": "1.6.2",
    "rollup-plugin-json": "4.0.0",
    "rollup-plugin-node-resolve": "5.2.0",
    "sinon": "9.2.4",
    "testdouble": "3.16.1"
  },
  "dependencies": {
    "@form8ion/cucumber-scaffolder": "1.4.0",
    "@form8ion/github-actions-node-ci": "1.13.4",
    "@form8ion/hapi-scaffolder": "2.0.3",
    "@form8ion/javascript-core": "2.9.1",
    "@form8ion/lift": "1.12.1",
    "@form8ion/lift-javascript": "1.9.1",
    "@form8ion/mocha-scaffolder": "1.0.0",
    "@form8ion/remark-plugin-scaffolder": "1.2.1",
    "@form8ion/remove-greenkeeper": "1.1.7",
    "@form8ion/renovate-scaffolder": "1.5.0",
    "@form8ion/replace-travis-ci-with-github-actions": "1.2.5",
    "@form8ion/scaffolder-scaffolder": "1.5.2",
    "@travi/github-scaffolder": "5.3.2",
    "@travi/javascript-scaffolder": "12.2.1",
    "@travi/project-scaffolder": "9.0.4",
    "@travi/travis-scaffolder-javascript": "4.0.1",
    "source-map-support": "0.5.19",
    "travis-token-updater": "1.6.12",
    "update-notifier": "5.1.0",
    "yargs": "16.2.0"
  }
}
