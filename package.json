{
  "name": "@form8ion/utils-cli",
  "description": "cli for various tools for the organization",
  "license": "MIT",
  "version": "0.0.0-semantically-released",
  "bin": {
    "form8ion-utils": "bin/form8ion-utils.js"
  },
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=12"
  },
  "files": [
    "bin/"
  ],
  "repository": "form8ion/utils-cli",
  "bugs": "https://github.com/form8ion/utils-cli/issues",
  "homepage": "https://npm.im/@form8ion/utils-cli",
  "author": "Matt Travi <npm@travi.org> (https://matt.travi.org/)",
  "scripts": {
    "test": "npm-run-all --print-label --parallel lint:* --parallel test:*",
    "test:unit": "nyc run-s test:unit:base",
    "test:unit:base": "DEBUG=any mocha 'src/**/*-test.js'",
    "coverage:report": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "test:integration": "run-s 'test:integration:base -- --profile noWip'",
    "test:integration:base": "DEBUG=any cucumber-js test/integration --profile base",
    "test:integration:debug": "DEBUG=test run-s test:integration",
    "test:integration:wip": "run-s 'test:integration:base -- --profile wip'",
    "test:integration:focus": "run-s 'test:integration:base -- --profile focus'",
    "lint:js": "eslint . --cache",
    "lint:md": "remark . --frail",
    "lint:sensitive": "ban",
    "lint:travis": "travis-lint .travis.yml",
    "lint:engines": "check-engine",
    "lint:gherkin": "gherkin-lint",
    "clean": "rimraf ./bin",
    "prebuild": "run-s clean",
    "build": "npm-run-all --print-label --parallel build:*",
    "build:js": "rollup --config",
    "watch": "run-s 'build:js -- --watch'",
    "prepack": "run-s build"
  },
  "devDependencies": {
    "@babel/node": "7.10.1",
    "@babel/register": "7.10.1",
    "@form8ion/babel-preset": "1.6.24",
    "@form8ion/commitlint-config": "1.0.9",
    "@form8ion/eslint-config": "1.6.21",
    "@form8ion/eslint-config-cucumber": "1.4.0",
    "@form8ion/eslint-config-mocha": "1.2.8",
    "@form8ion/remark-lint-preset": "1.0.7",
    "@travi/any": "2.0.9",
    "ban-sensitive-files": "1.9.7",
    "chai": "4.2.0",
    "check-engine": "1.8.1",
    "codecov": "3.7.0",
    "cucumber": "6.0.5",
    "cz-conventional-changelog": "3.2.0",
    "gherkin-lint": "4.0.1",
    "husky": "4.2.5",
    "mocha": "8.0.1",
    "npm-run-all": "4.1.5",
    "nyc": "15.1.0",
    "remark-cli": "8.0.0",
    "rimraf": "3.0.2",
    "rollup": "2.16.0",
    "rollup-plugin-auto-external": "2.0.0",
    "rollup-plugin-executable": "1.6.0",
    "rollup-plugin-json": "4.0.0",
    "rollup-plugin-node-resolve": "5.2.0",
    "sinon": "9.0.2",
    "travis-lint": "1.0.0"
  },
  "dependencies": {
    "@form8ion/cucumber-scaffolder": "1.1.0",
    "@form8ion/hapi-scaffolder": "2.0.0",
    "@form8ion/lift": "1.9.2",
    "@form8ion/lift-javascript": "1.2.0",
    "@form8ion/remark-plugin-scaffolder": "1.1.0",
    "@form8ion/remove-greenkeeper": "1.1.4",
    "@form8ion/renovate-scaffolder": "1.3.0",
    "@travi/github-scaffolder": "5.1.2",
    "@travi/javascript-scaffolder": "10.22.6",
    "@travi/project-scaffolder": "8.10.3",
    "@travi/travis-scaffolder-javascript": "3.2.1",
    "source-map-support": "0.5.19",
    "travis-token-updater": "1.6.12",
    "update-notifier": "4.1.0",
    "yargs": "15.3.1"
  }
}
